<div class="header">
	Check out history
</div>


<table>
  <tr>
    <th>User</th>
    <th>Terriory</th>
    <th>Publisher</th>
    <th>Checked out</th>
    <th>Checked in</th>
    <th>Completed with</th>
    <th>Notes</th>
  </tr>

<% @checkouts.each do |checkout| %>
  <tr>
    <td><%=h checkout.user_id %></td>
    <td><%=h checkout.territory.name %></td>
    <td><%=h checkout.publisher.name %></td>
    <td><%=h checkout.checked_out.strftime("%m-%d-%Y") %></td>
    <td><%= if checkout.checked_in == nil then checkout.checked_in else checkout.checked_in.strftime("%m-%d-%Y") end %></td>
    <td><%=h checkout.completed_with %></td>
    <td><%=h checkout.notes %></td>
    <td><%= link_to 'Show', checkout %></td>
    <td><%= if checkout.checked_in == nil then link_to 'Edit/Check', edit_checkout_path(checkout) else link_to 'Edit', edit_checkout_path(checkout) end     %></td>
    <td><%= link_to 'Destroy', checkout, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New checkout', new_checkout_path %>