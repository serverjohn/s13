<h1>New Worked With Type</h1>
  <div class="form">
  <%= form_for(@worked_with_type) do |f| %>
    <%= render "shared/error_messages", :target => @worked_with_type %>

    <h2><%= f.label :name %></h2>
    <div><%= f.text_field :name %></div>
    <%= f.hidden_field :active, :value => "Y"%>

    <%= f.submit 'Create' %>
  <% end %>
</div>