<b>New territory</b>

<%= form_for @territory, :html => {:multipart => true} do |f| %>
  <%= render "shared/error_messages", :target => @territory %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :territory_type_id, "Territory Type" %><br />
    <%= f.select :territory_type_id, options_for_select(territory_types) %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :maps %><br />
    <%= f.file_field :maps %>
  </p>
  <p>
    <%= f.label :notes %><br />
    <%= f.text_field :notes %>
  </p>
  <p>
	<%= f.hidden_field :active, :value => "Y"%>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', territories_path %>