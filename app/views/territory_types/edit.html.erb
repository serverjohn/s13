<h1>Edit Territory Type</h1>

<div class="form">
<%= form_for(@territory_type) do |f| %>
  <%= render "shared/error_messages", :target => @territory_type %>

  <h2><%= f.label :name %></h2>
  <div><%= f.text_field :name %></div>


  <h2><%= f.label :active %></h2>
  <div>
    <%= f.radio_button :active, 'Y' %> 
    <%= label :active, 'Yes' %>
  </div>
  <div>
    <%= f.radio_button :active, 'N' %>
    <%= label :active, 'No' %>
  </div>

    <%= f.submit 'Save', :class => "form-button" %>
<% end %>
</div>
