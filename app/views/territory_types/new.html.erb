<h1>New Territory Type</h1>
<div class="form">
  <%= form_for(@territory_type) do |f| %>
    <%= render "shared/error_messages", :target => @territory_type %>

    <h2><%= f.label :name %></h2>
    <div><%= f.text_field :name %></div>
    
    <%= f.hidden_field :active, :value => "Y"%>
    
    <%= f.submit 'Save' %>
  <% end %>
</div>